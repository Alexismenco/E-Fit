<style>
.oval-line {
  width: 100%;
  height: 150px;
  border-bottom-left-radius: 40%;
  border-bottom-right-radius: 40%;
  background-color: rgb(18, 239, 18);
  border-bottom: 2px solid #000;


}

.container {
        position: relative;
        display: inline-block;
        margin-top: 50px;
        text-decoration: none;
    }

.container span {
    position: absolute;
    top: 20%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 85%;
    text-align: center;
    color: white;
    font-weight: 800;
    font-size: 25px;
    -webkit-text-stroke: 1px black;
}

.container p {
    position: absolute;
    top: 60%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 65%;
    text-align: center;
    color: white;
    font-weight: 800;
    font-size: 20px;
    -webkit-text-stroke: 1px black;
    margin-top: -25px;
}

.container .segundo {
    position: absolute;
    top: 80%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 85%;
    text-align: center;
    color: white;
    font-weight: 800;
    font-size: 20px;
    -webkit-text-stroke: 1px black;
}


.container img {
    width: 100%;
    border-radius: 50px;
    box-shadow: 0 22px 20px rgba(62, 128, 125, 0.5);
}

@media (min-width: 800px) {
    .container img{
        width: 75%;
    }
  }
@media (min-width: 700px) {
    .cont-img{
        margin-left: 300px !important;
    }
     .cont-img2{
        margin-right: 300px !important;
    }
  }

  /* Video */
  .video-iframe {
        width: 100vw; /* Ancho igual al ancho de la ventana del navegador */
        height: 100vh; /* Altura igual a la altura de la ventana del navegador */
        border: none; /* Elimina el borde del iframe */
    }

.video-container {
  margin: 0;
}

.video-container ul {
  padding: 0;
}
.custom-video {
  width: 100%;
}
.custom-video::-webkit-media-controls-play-button {
  background-color: rgba(0, 0, 0, 0.3);
  color: #000;
}

.custom-video::-webkit-media-controls-seek-back-button,
.custom-video::-webkit-media-controls-seek-forward-button {
  background-color: rgba(0, 0, 0, 0.3);
  color: #000;
}

.custom-video::-webkit-media-controls-volume-slider {
  background-color: rgba(0, 0, 0, 0.3);
}

.custom-video::-webkit-media-controls-mute-button {
  background-color: rgba(0, 0, 0, 0.3);
  color: #000;
}

video {
  border-bottom: 1px solid #10c716;
}

</style>

